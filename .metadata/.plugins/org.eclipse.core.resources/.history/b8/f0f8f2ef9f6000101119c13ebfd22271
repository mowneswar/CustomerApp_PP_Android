	@Test
	public void Launch_Hourly_DIA_T1_Cash() throws Exception {
		Launch_Hourly_DIA_T1_Cash Hourly_DIA_T1_Cash = new Launch_Hourly_DIA_T1_Cash();
		Hourly_DIA_T1_Cash.testOnHourlyHomescreen();
