		Hourly_DIA_T1_Cash.testOnchooseyourcarPage();
		Hourly_DIA_T1_Cash.testOnyourRidePageCash();
		Hourly_DIA_T1_Cash.testOnHourlyselectDurationPage();
		Hourly_DIA_T1_Cash.testOnpickupScreen();
		Hourly_DIA_T1_Cash.testOnHourlysManageYourPage();
		Hourly_DIA_T1_Cash.testOnquicfeedbackScreen();
		Hourly_DIA_T1_Cash.testOnexperiencefeedbackScreen();
		System.out.println("✅ Case 2 : Hourly booking validated — Customer successfully booked a ride from Dubai airport Terminal 1 with 3 add on stops and completed the journey with cash payment.");
		Thread.sleep(50000);
	}
